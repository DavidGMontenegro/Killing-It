        <Image Source="/Images/ammo.png" Height="30" Width="30" Canvas.Left="30" Canvas.Top="16" HorizontalAlignment="Center" VerticalAlignment="Top" ></Image>
        <Image Source="/Images/golden frame.png"  Width="107" Height="48" Stretch="Fill" Canvas.Left="10" Canvas.Top="7" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <ProgressBar Name="liveProgressBar" Height="24" Width="211" Canvas.Left="467" Canvas.Top="21" HorizontalAlignment="Left" VerticalAlignment="Center" Background="Wheat" Foreground="#3a260e" Minimum="0" Maximum="100" Value="100"/>

    </Canvas>

</Window>
